{
    "name": "danboehm/articulant",
    "description": "Useful extensions to Laravel's Eloquent ORM",
    "keywords": ["laravel", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Boehm",
            "email": "dboehm.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.33",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4":{
            "DanBoehm\\Articulant\\": "src/Articulant"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanBoehm\\Articulant\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanBoehm\\Articulant\\ArticulantServiceProvider"
            ]
        }
    }
}
